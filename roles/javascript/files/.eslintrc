{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "2018",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "no-console": 0,
    "dot-notation": 0,
    "camelcase": 0
  }
}
